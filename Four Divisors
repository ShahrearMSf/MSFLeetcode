from typing import List
import math

class Solution:
    def sumFourDivisors(self, nums: List[int]) -> int:
        total_sum = 0
        
        for n in nums:
            div_sum = 0
            div_count = 0
            
            # Check divisors up to sqrt(n)
            for i in range(1, int(math.sqrt(n)) + 1):
                if n % i == 0:
                    j = n // i
                    
                    if i == j:
                        div_count += 1
                        div_sum += i
                    else:
                        div_count += 2
                        div_sum += i + j
                    
                    # Early stop if more than 4 divisors
                    if div_count > 4:
                        break
            
            if div_count == 4:
                total_sum += div_sum
        
        return total_sum
