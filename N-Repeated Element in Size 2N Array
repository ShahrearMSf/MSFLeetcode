from typing import List

class Solution:
    def repeatedNTimes(self, nums: List[int]) -> int:
        for i in range(len(nums) - 1):
            if nums[i] == nums[i + 1]:
                return nums[i]
            if i + 2 < len(nums) and nums[i] == nums[i + 2]:
                return nums[i]
            if i + 3 < len(nums) and nums[i] == nums[i + 3]:
                return nums[i]
